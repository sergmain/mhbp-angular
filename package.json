{
  "name": "metaheuristic-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build",
    "build2": "ng build --baseHref=/aiai-app/ --prod=true",
    "build3": "ng build --prod=true",
    "build4": "ng build --aot --prod=true",
    "build5": "ng build --aot",
    "build6": "ng update @angular/core@15 @angular/cli@15",
    "npm-update": "ng update @angular/core @angular/cli",
    "build7": "ng update @angular/material@11",
    "build8": "npm install -g npm@9.4.2",
    "npm-install-g": "npm install -g @angular/cli@latest",
    "npm-install": "npm install",
    "npm-install-force": "npm install --force",
    "ng add @angular/material": "ng add @angular/material",
    "npm-audit": "npm audit",
    "npm-audit-fix": "npm audit fix",
    "npm-audit-fix-force": "npm audit fix --force",
    "npm-install2": "npm install -g npm",
    "npm-i-package-lockg": "npm i --package-lock-only",
    "ng add @angular-eslint/schematics": "ng add @angular-eslint/schematics",
    "ng g @angular-eslint/schematics:convert-tslint-to-eslint": "ng g @angular-eslint/schematics:convert-tslint-to-eslint metaheuristic-app-e2e",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "ng version",
    "extract-i18n": "ngx-translate-extract  -p /**/*.html /**/*.ts /**/*.pug --input ./src --output ./src/assets/i18n/EN.json ./src/assets/i18n/RU.json --clean --sort --format json --marker marker",
    "ng--migrate-only--from12.0.0 --to 13.0.0": "ng update @angular/cdk --migrate-only --from 12.0.0 --to 13.0.0",
    "sass-migrator namespace **/*.scss": "sass-migrator -n namespace **/*.scss"
  },
  "engines": {
    "node": "v18.14.0",
    "npm": "9.5.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "core-js": "2.6.9",
    "file-saver": "2.0.5",
    "jszip": "3.10.1",
    "normalize.css": "8.0.1",
    "rxjs": "6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@biesbjerg/ngx-translate-extract": "7.0.4",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "typescript": "4.9.5"
  }
}